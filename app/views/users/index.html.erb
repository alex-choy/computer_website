
<div style="margin: auto; padding-left: 25px;">
  <h1>All Users</h1>

  <table>
    <tr>
      <th> Username </th>
      <th> Email </th>
      <th> Password </th>
    </tr>

    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.password_digest %></td>
      </tr>

    <% end %>
  </table>

  <%= link_to 'New User', new_user_path %>
</div>
